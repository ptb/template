{
  "author": {
    "email": "peter@ptb.dev",
    "name": "Peter T Bosse II",
    "url": "https://ptb.dev/"
  },
  "browserslist": [
    "> 1% in US",
    "Last 2 versions",
    "Android >= 71",
    "Chrome >= 49",
    "Edge >= 16",
    "Firefox >= 52",
    "IE >= 11",
    "iOS >= 9",
    "Safari >= 9",
    "Samsung >= 7.4"
  ],
  "dependencies": {
    "@amory/style": "2020.2.1-0",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@koa/router": "8.0.8",
    "@next/bundle-analyzer": "9.3.4",
    "babel-plugin-lodash": "3.3.4",
    "gatsby": "2.20.10",
    "koa": "2.11.0",
    "lodash": "4.17.15",
    "next": "9.3.4",
    "next-build-id": "3.0.0",
    "next-compose-plugins": "2.2.0",
    "next-routes": "1.4.2",
    "next-seo": "4.4.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@hot-loader/react-dom": "16.13.0",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "@types/react": "16.9.31",
    "@welldone-software/why-did-you-render": "6.2.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "npm-run-all": "4.1.5",
    "regenerator-runtime": "0.13.5",
    "typescript": "3.8.3",
    "webpack": "4.42.1"
  },
  "engines": {
    "node": "12.10.0"
  },
  "eslintConfig": {
    "extends": [
      "./.eslint/airbnb/index.json",
      "./.eslint/json-style.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js{,x}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "babel",
    "printWidth": 78,
    "proseWrap": "always",
    "requirePragma": false,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "private": true,
  "renovate": {
    "automerge": true,
    "extends": [
      "config:base"
    ]
  },
  "scripts": {
    "build": "npx npm-run-all --parallel build:*",
    "build:gatsby": "gatsby build",
    "build:next": "ANALYZE=true NODE_ENV=production next build",
    "build:storybook": "build-storybook -c ./.storybook -o ./doc",
    "clean": "npx npm-run-all --serial clean:next clean:gatsby clean:node",
    "clean:gatsby": "gatsby clean",
    "clean:next": "rm -rf tmp",
    "clean:node": "rm -rf node_modules yarn.lock",
    "lint": "eslint --ext js,jsx --fix '*.js{,x}' 'src/**/*.js{,x}'",
    "serve": "npx npm-run-all --parallel serve:*",
    "serve:gatsby": "gatsby serve",
    "serve:next": "NODE_ENV=production node --experimental-modules server.js",
    "start": "npx npm-run-all --parallel start:*",
    "start:gatsby": "gatsby develop",
    "start:next": "node --experimental-modules server.js",
    "start:storybook": "start-storybook -c ./.storybook -p 9000 -s ./public"
  },
  "type": "module",
  "version": "1.0.0"
}
